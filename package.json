{
  "name": "vue3.dev",
  "version": "0.1.0",
  "private": true,
  "description": "Website for vue3-ui",
  "keywords": [],
  "homepage": "https://github.com/pathscale/vue3.dev",
  "bugs": "https://github.com/pathscale/vue3.dev/issues",
  "repository": "https://github.com/pathscale/vue3.dev",
  "license": "UNLICENSED",
  "author": "PathScale (https://vue3.dev/)",
  "contributors": [],
  "scripts": {
    "prebuild": "shx rm -rf dist && shx mkdir dist && npm run build:snippets",
    "build": "rollup -c --environment NODE_ENV:production",
    "postbuild": "shx cp serve.json dist",
    "build:snippets": "node build/snippets.js",
    "fetch-bundle": "./fetchBundle.sh",
    "css-lint": "stylelint src",
    "lint": "eslint --ext .js,.ts,.vue,.html src --fix --max-warnings=0 .",
    "prestart": "shx rm -rf dist && npm run build:snippets",
    "start": "rollup -c -w --environment NODE_ENV:development",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{css,vue}": "stylelint",
    "**/*.{js,ts,vue,md,html}": "eslint --cache --fix"
  },
