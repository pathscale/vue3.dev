{
  "name": "vue3.dev",
  "version": "0.1.0",
  "private": true,
  "description": "Website for vue3-ui",
  "keywords": [],
  "homepage": "https://github.com/pathscale/vue3.dev",
  "bugs": "https://github.com/pathscale/vue3.dev/issues",
  "repository": "https://github.com/pathscale/vue3.dev",
  "license": "UNLICENSED",
  "author": "PathScale (https://vue3.dev/)",
  "contributors": [],
  "scripts": {
    "prebuild": "shx rm -rf dist && shx mkdir dist && bun run build:snippets",
    "build": "rollup -c --environment NODE_ENV:production",
    "postbuild": "shx cp serve.json dist",
    "build:snippets": "bun build/snippets.js",
    "fetch-bundle": "./fetchBundle.sh",
    "css-lint": "stylelint src",
    "lint": "eslint --ext .js,.mjs,.ts,.vue src --fix --max-warnings=0 .",
    "prestart": "shx rm -rf dist && bun run build:snippets",
    "start": "rollup -c -w --environment NODE_ENV:development",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{css,vue}": "stylelint",
    "**/*.{js,ts,vue,md,html}": "eslint --cache --fix"
  },
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@bulvar/bulma": "^0.11.24",
    "@commitlint/config-conventional": "^17.0.3",
    "@pathscale/appstate-fast": "^0.0.4",
    "@pathscale/bulma-extensions-css-var": "0.1.1",
    "@pathscale/fonts-metroclean": "^1.0.8",
    "@pathscale/frappe-charts": "^0.0.2",
    "@pathscale/rollup-plugin-vue3-ui-css-purge": "^0.0.41",
    "@pathscale/vue3-svg-icons": "^0.0.12",
    "@pathscale/vue3-ui": "^1.0.14",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-sucrase": "^4.0.4",
    "@vitejs/plugin-vue": "5.2.3",
    "@vue/compiler-sfc": "^3.5.13",
    "autoprefixer": "^10.4.21",
    "babel-plugin-module-resolver": "^4.1.0",
    "bulma-calendar": "^6.1.18",
    "dotenv": "^16.0.1",
    "eslint": "^9.23.0",
    "eslint-config-ash-nazg": "^37.1.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-babel-module": "5.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "highlight.js": "^11.5.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.4.1",
    "prettier": "^3.5.3",
    "rollup": "^2.75.7",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-gzip": "^3.0.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "tinycolor2": "1.6.0",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.28.0",
    "vue": "^3.5.13",
    "vue-composable": "^1.0.0-beta.24",
    "vue-router": "^4.5.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
