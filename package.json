  "scripts": {
    "prebuild": "shx rm -rf dist && shx mkdir dist && bun run build:benchie && bun run build:snippets",
    "build": "rollup -c --environment NODE_ENV:production",
    "postbuild": "shx cp serve.json dist",
    "build:snippets": "bun build/snippets.js",
    "build:benchie": "bun run cdn/build-benchie.ts",
    "fetch-bundle": "./fetchBundle.sh",
    "prestart": "shx rm -rf dist && bun run build:benchie && bun run build:snippets",
    "start": "rollup -c -w --environment NODE_ENV:development",
    "lint": "biome lint --write .",
    "lint:fix": "biome check --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,vue,json}": ["biome check --write", "biome lint --write"]
  },
  "devDependencies": {
    "@babel/core": "7.26.10",
